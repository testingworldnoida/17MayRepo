package filehanding;

import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileReader;

public class ReadDataFromFile {

	
	public void readData() throws FileNotFoundException{
		File f = new File("D://TestData.txt");
		FileReader fr = new FileReader(f);
		int i = fr.read();   // ASCII
	}
	
}
